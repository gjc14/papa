{
	"name": "papa",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"dev": "react-router dev",
		"start": "react-router-serve build/server/index.js",
		"typecheck": "react-router typegen && tsc",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"init": "tsx scripts/init.ts && pnpm run db:push && tsx scripts/init-object-storage.ts && tsx scripts/init-admin.ts && tsx scripts/init-fin.ts && pnpm run dev",
		"create-plugin:admin": "tsx scripts/create-admin-plugin.ts",
		"create-plugin:web": "tsx scripts/create-web-plugin.ts",
		"format": "prettier --write .",
		"db:push": "pnpm exec drizzle-kit push",
		"auth:generate": "pnpm exec @better-auth/cli generate --config app/lib/auth/auth.server.ts --output app/lib/db/schema/auth.ts",
		"convert:logo": "cd ./public && bash -c 'for size in 512 300 100 32; do ffmpeg -i logo.png -vf scale=$size:-1 logo-$size.png; done'",
		"convert:favicon": "cd ./public && bash -c 'ffmpeg -i logo.png -vf \"scale=32:32:force_original_aspect_ratio=decrease,pad=32:32:(ow-iw)/2:(oh-ih)/2:color=0x00000000\" -y favicon.ico'"
	},
